{
    "name": "puli/puli",
    "description": "Puli manages files, directories and other resources in a filesystem-like repository.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9"
    },
    "require-dev": {
        "symfony/filesystem": "~2.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Puli\\": "src/",
            "Puli\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
